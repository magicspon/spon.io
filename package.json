	"quokka": {
		"babel": {
			"env": "test",
			"configFile": "./.babelrc.js",
			"polyfill": true
		},
		"installPackageCommand": "yarn add {packageName}",
		"plugins": [
			"jsdom-quokka-plugin"
		],
		"env": {
			"params": {
				"env": "NODE_ENV=test"
			}
		}
	},
	"scripts": {
		"dev": "gatsby develop",
		"start": "npm run dev",
		"storybook": "STORYBOOK_ENV=storybook start-storybook -s ./static -p 6006",
		"storybook:build": "STORYBOOK_ENV=storybook build-storybook -s ./static -c .storybook -o docs",
		"build": "gatsby build",
		"test": "jest",
		"test:watch": "jest --watch",
		"coverage": "jest --coverage",
		"lint:js": "eslint --ext src/**/*.js",
		"lint:css": "stylelint './src/**/*.css'",
		"pretty": "prettier 'src/**/*.{js,jsx,css}' --write",
		"cy:run": "cypress run",
		"cy:open": "cypress open"
